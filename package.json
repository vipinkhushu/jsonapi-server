{
  "name": "jagapi",
  "version": "5.6.0",
  "description": "A config driven NodeJS framework implementing json:api",
  "keywords": [
    "jsonapi",
    "json:api",
    "swagger",
    "api"
  ],
  "main": "lib/jsonApi.js",
  "types": "types/jsonApi.d.ts",
  "author": "Arnav Gupta <arnav@codingblocks.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/coding-blocks/jsonapi-server"
  },
  "engines": {
    "node": "8",
    "npm": "5",
    "yarn": "1"
  },
  "greenkeeper": {
    "ignore": [
      "express-graphql",
      "graphql"
    ]
  },
  "dependencies": {
    "@types/express": "^4.11.1",
    "@types/joi": "^13.0.5",
    "async": "2.6.0",
    "body-parser": "1.18.2",
    "cookie-parser": "1.4.3",
    "debug": "3.1.0",
    "express": "4.16.2",
    "express-graphql": "^0.6.12",
    "graphql": "^0.13.1",
    "joi": "^13.1.1",
    "lodash.assign": "4.2.0",
    "lodash.isequal": "4.5.0",
    "lodash.omit": "4.5.0",
    "lodash.pick": "4.4.0",
    "lodash.uniq": "4.5.0",
    "lodash.uniqby": "4.7.0",
    "qs": "6.5.1",
    "request": "2.83.0",
    "use-strict": "1.0.1",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "coveralls": "3.0.0",
    "eslint": "4.18.2",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-node": "5.2.1",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-standard": "3.0.1",
    "jscpd": "0.6.17",
    "lokka": "1.7.0",
    "lokka-transport-http": "1.6.1",
    "mocha": "5.0.1",
    "mocha-performance": "0.1.1",
    "nyc": "11.3.0",
    "swagger-tools": "0.10.3",
    "typedoc": "^0.10.0",
    "typedoc-plugin-external-module-name": "^1.1.1",
    "v8-profiler": "5.7.0"
  },
  "scripts": {
    "start": "node example/server.js",
    "test": "mocha",
    "cover": "nyc mocha",
    "coverage": "[ -n \"$TRAVIS\" ] && nyc report --reporter=text-lcov | coveralls || nyc report --reporter=html",
    "performance": "node --allow-natives-syntax --harmony ./node_modules/mocha/bin/_mocha -S --reporter mocha-performance ./test/*.js",
    "lint": "eslint ./example ./lib ./test --quiet && echo 'âœ” All good!'",
    "jscpd": "jscpd --blame -p ./lib/ || echo 'Finished!'",
    "ci": "npm run jscpd && npm run lint && npm run test",
    "predocs": "rimraf docs/public/*",
    "docs": "typedoc types"
  }
}
